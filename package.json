{
  "name": "@nofwl/i18n",
  "version": "0.0.2",
  "description": "The internationalization (i18n) library for JavaScript. Support for i18n of text, images, placeholders, styles",
  "main": "dist/i18n.umd.js",
  "module": "dist/i18n.esm.js",
  "author": "lencx <cxin1314@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "serve": "http-server",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "i18n",
    "internationalization",
    "text",
    "images",
    "style",
    "placeholder"
  ],
  "bugs": {
    "url": "https://github.com/nofwl/i18n/issues"
  },
  "homepage": "https://github.com/nofwl/i18n",
  "repository": "git@github.com:nofwl/i18n.git",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-config-babel": "^8.0.1",
    "eslint-plugin-flowtype": "^3.1.4",
    "http-server": "^0.11.1",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.3",
    "lodash.camelcase": "^4.3.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-terser": "^3.0.0"
  }
}
